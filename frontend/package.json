{
  "name": "miranda-portfolio-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Miranda Liang's portfolio frontend - A terminal-themed React application showcasing projects and experience",
  "author": {
    "name": "Miranda Liang",
    "email": "miranda@buildsomething.epic",
    "url": "https://mirandaliang.com"
  },
  "license": "MIT",
  "homepage": "https://mirandaliang.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mirandaliang/portfolio.git",
    "directory": "frontend"
  },
  "bugs": {
    "url": "https://github.com/mirandaliang/portfolio/issues"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "vite",
    "tailwind",
    "terminal-theme",
    "full-stack"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-query-devtools": "^4.35.0",
    "axios": "^1.5.0",
    "framer-motion": "^10.16.0",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.4.0",
    "react-error-boundary": "^4.0.11",
    "lucide-react": "^0.263.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-react": "^4.0.3",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.16.4",
    "vite-bundle-analyzer": "^0.7.0",
    "typescript": "^5.0.2",
    "tailwindcss": "^3.3.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28",
    "@tailwindcss/typography": "^0.5.9",
    "@tailwindcss/forms": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "vitest": "^0.34.0",
    "jsdom": "^22.1.0",
    "@vitest/ui": "^0.34.0",
    "@vitest/coverage-v8": "^0.34.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@playwright/test": "^1.37.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:run"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "sideEffects": [
    "**/*.css"
  ]
}
